{
  "name": "revelation-visualized",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run build-index && npm run build-explanation",
    "now-build": "npm run create-symlinks && npm run build",
    "build-index": "browserify -t [ sveltify --extensions [.html] ] client/index.js -o public/build.js",
    "watch-index": "watchify -t [ sveltify --extensions [.html] ] client/index.js -o public/build.js -d",
    "build-explanation": "browserify -t [ sveltify --extensions [.html] ] client/explanation-index.js -o public/explanation-build.js",
    "watch-explanation": "watchify -t [ sveltify --extensions [.html] ] client/explanation-index.js -o public/explanation-build.js -d",
    "create-symlinks": "mkdir client/node_modules/; cd client/node_modules; ln -s ../component/ component && ln -s ../lib/ lib; cd ../../;",
    "test": "echo \"No test script found :-(\"",
    "start": "serve public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/revelation-visualized.git"
  },
  "keywords": [],
  "author": "",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/revelation-visualized/issues"
  },
  "homepage": "https://github.com/TehShrike/revelation-visualized#readme",
  "dependencies": {
    "browserify": "14.1.0",
    "eventemitter3": "2.0.2",
    "majority-text-family-35-revelation": "1.0.0",
    "multi-part-range-compare": "1.0.1",
    "one-to-many-array-zip": "~1.0.2",
    "revelation": "~2.0.1",
    "serve": "5.0.2",
    "svelte": "1.11.1",
    "sveltify": "1.0.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.22.0",
    "babelify": "7.3.0",
    "watchify": "3.9.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015"
        }
      ]
    ]
  }
}
